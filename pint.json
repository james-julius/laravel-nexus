{
    "preset": "laravel",
    "rules": {
        "simplified_null_return": false,
        "not_operator_with_successor_space": true,
        "concat_space": {
            "spacing": "one"
        },
        "ordered_imports": {
            "sort_algorithm": "alpha"
        },
        "phpdoc_separation": false,
        "no_unused_imports": true
    }
}